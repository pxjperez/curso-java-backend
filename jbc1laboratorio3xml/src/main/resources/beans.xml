<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    
    <bean id="holaMundo" class="edu.cibertec.beans.HolaMundo" scope="prototype"
    init-method="iniciar" destroy-method="destruir">
        <property name="nombre" value="JUAN CALROS"></property>
    </bean>

    <bean id="holaMundoCicloVida" class="edu.cibertec.beans.HolaMundo"
          init-method="iniciar" destroy-method="destruir">
        <property name="nombre" value="JUAN CALROS"></property>
    </bean>
</beans>
